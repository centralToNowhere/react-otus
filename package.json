{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "Game of life on React",
  "main": "src/index.tsx",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --config ./webpack-dev.config.js",
    "build:prod": "webpack --config ./webpack-prod.config.js",
    "types": "tsc --noEmit",
    "test": "jest --silent --coverage && jest-coverage-badges && git add coverage/badge*",
    "check": "npm run types && npm run test && npm run lint && npm run loki:test && npm run build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "build-storybook",
    "serve": "webpack serve --config ./webpack-dev.config.js  --entry ./src/index.tsx",
    "serve:prod": "webpack serve --config ./webpack-prod.config.js  --entry ./src/index.tsx",
    "loki:test": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "pre-push": "npm run check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centralToNowhere/react-otus.git"
  },
  "author": "Dmitry Rozhkov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/centralToNowhere/react-otus/issues"
  },
  "homepage": "https://github.com/centralToNowhere/react-otus/tree/develop",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/jest": "^11.7.1",
    "@redux-saga/testing-utils": "^1.1.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.4",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@webpack-cli/generators": "^2.4.1",
    "@webpack-cli/init": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-rewire-ts": "^1.4.0",
    "core-js": "^3.18.3",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "loki": "^0.28.1",
    "mini-css-extract-plugin": "^2.4.4",
    "node-fetch": "^3.1.0",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "redux-saga-test-plan": "^4.0.4",
    "typescript": "^4.5.5",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@reduxjs/toolkit": "^1.7.1",
    "bootstrap": "^5.1.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux-saga": "^1.1.3"
  },
  "browserslist": [
    "defaults"
  ],
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
